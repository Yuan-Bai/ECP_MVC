<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>GetGoodsList</servlet-name>
    <servlet-class>com.rgxz.ecp.servlet.GetGoodsList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.rgxz.ecp.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.rgxz.ecp.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetRGServlet</servlet-name>
    <servlet-class>com.rgxz.ecp.servlet.GetRGServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetGoodsList</servlet-name>
    <url-pattern>/getgoodslist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetRGServlet</servlet-name>
    <url-pattern>/randomGoods</url-pattern>
  </servlet-mapping>
</web-app>
